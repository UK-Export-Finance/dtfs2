{
  "name": "dtfs2",
  "version": "1.0.0",
  "description": "DTFS2 Portal API",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "start": "node src/index.js",
    "start:dev": "npx nodemon src/index.js",
    "start_azure_debug": "node --inspect=0.0.0.0:$APPSVC_TUNNEL_PORT src/index.js",
    "start_azure_debug_fileshare": "node fileshare-test.js",
    "unit-test": "jest --notify --coverage --verbose --config=unit.jest.config.js ",
    "unit-test-quick": "jest --config=unit.jest.config.js --testTimeout=10000",
    "api-test": "jest --notify --coverage --verbose --runInBand --config=api-test.jest.config.js",
    "api-test-file": "jest --config=api-test-file.jest.config.js --runInBand --verbose --testMatch",
    "api-test-quick": "jest --config=api-test.jest.config.js --runInBand --testTimeout=10000",
    "api-test-gef": "jest --config=api-test-gef.jest.config.js --coverage --runInBand --testTimeout=10000 --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs2.git"
  },
  "keywords": [
    "export",
    "finance"
  ],
  "author": "UK Export Finance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs2/issues"
  },
  "homepage": "https://github.com/UK-Export-Finance/dtfs2#readme",
  "engineStrict": true,
  "engines": {
    "node": ">=14.0.0 <15.0.0",
    "npm": ">=6.0.0 <7.0.0"
  },
  "dependencies": {
    "@azure/storage-file-share": "12.8.0",
    "@graphql-tools/schema": "^8.3.1",
    "@sentry/integrations": "^6.15.0",
    "@sentry/node": "^6.15.0",
    "apollo-server-express": "2.18.2",
    "apollo-server-testing": "^2.25.3",
    "axios": "^0.24.0",
    "compression": "^1.7.4",
    "cors": "2.8.5",
    "date-fns": "^2.25.0",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "filesize": "^8.0.5",
    "graphql": "^15.0.0",
    "graphql-middleware": "^4.0.2",
    "helmet": "^4.6.0",
    "is-port-reachable": "^3.0.0",
    "jsonwebtoken": "8.5.1",
    "moment": "2.29.1",
    "moment-timezone": "0.5.34",
    "mongo-dot-notation": "2.0.0",
    "mongodb": "~3.6.12",
    "multer": "1.4.3",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.1",
    "node-notifier": "^10.0.0",
    "notifications-node-client": "^5.1.0",
    "passport": "0.5.0",
    "passport-jwt": "4.0.0",
    "request": "^2.88.2",
    "xss": "^1.0.10"
  },
  "devDependencies": {
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^27.4.3",
    "nodemon": "^2.0.15",
    "supertest": "6.1.6"
  }
}
