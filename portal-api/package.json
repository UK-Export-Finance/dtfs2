{
  "name": "portal-api",
  "version": "1.0.0",
  "private": true,
  "description": "DTFS Portal API",
  "keywords": [
    "export",
    "finance"
  ],
  "homepage": "https://github.com/UK-Export-Finance/dtfs2#readme",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs2.git"
  },
  "license": "MIT",
  "author": "UK Export Finance",
  "main": "server/index.ts",
  "scripts": {
    "api-test": "jest --coverage --verbose --runInBand --forceExit --clearMocks --config=api-test.jest.config.js",
    "api-test-dev": "jest --verbose --runInBand --forceExit --clearMocks --config=api-test.jest.config.js",
    "lint": "eslint --color **/*.{ts,js} --ignore-path ../.eslintignore",
    "lint:fix": "eslint --fix --color **/*.{ts,js}",
    "prettier": "prettier --no-error-on-unmatched-pattern --check **/*.{ts,js} --ignore-path ../.prettierignore",
    "prettier:fix": "prettier --write **/*.{ts,js} --ignore-path ../.prettierignore",
    "start": "npx ts-node server/index.ts",
    "start:dev": "npx nodemon server/index.ts --config ../nodemon.json",
    "type-check": "tsc --noEmit --pretty",
    "unit-test": "jest --coverage --verbose --config=unit.jest.config.js",
    "unit-test-dev": "jest --verbose --config=unit.jest.config.js",
    "unit-test-ff": "jest --coverage --verbose --config=unit.ff.jest.config.js --passWithNoTests"
  },
  "dependencies": {
    "@azure/storage-file-share": "12.29.1",
    "@ukef/dtfs2-common": "1.0.0",
    "axios": "1.12.0",
    "comma-number": "^2.1.0",
    "compression": "^1.8.1",
    "cors": "2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "16.0.3",
    "escape-string-regexp": "^4.0.0",
    "express": "^4.22.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.3.1",
    "filesize": "^8.0.7",
    "immer": "^10.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.3",
    "lodash": "^4.17.23",
    "mongodb": "^4.17.2",
    "multer": "2.0.2",
    "node-fetch": "^2.7.0",
    "passport": "0.7.0",
    "passport-jwt": "^4.0.1",
    "sanitize-html": "^2.17.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "validator": "^13.15.23",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.14",
    "@types/jest-when": "^3.5.5",
    "@types/lodash.orderby": "^4.6.9",
    "@types/supertest": "^6.0.2",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.7.0",
    "jest-when": "^3.7.0",
    "lodash": "^4.17.23",
    "node-mocks-http": "^1.17.2",
    "prettier": "^3.7.4",
    "supertest": "6.3.3",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=24.1.0",
    "npm": ">=11.3.0"
  },
  "engineStrict": true
}
