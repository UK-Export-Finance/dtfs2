{% from "govuk/components/back-link/macro.njk" import govukBackLink %}
<!DOCTYPE html>
<html lang="en" class="govuk-template ">
  <head>
    <meta charset="utf-8"/>
    <title>
      {% block pageTitle %}{% endblock %}, Trade Finance Manager, gov.uk</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover">
    <meta name="theme-color" content="#0b0c0c"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <link rel="shortcut icon" sizes="16x16 32x32 48x48" href="/assets/images/favicon.ico?v=2" type="image/x-icon"/>
    <link rel="mask-icon" href="/assets/images/govuk-mask-icon.svg" color="#0b0c0c">
    <link rel="apple-touch-icon" sizes="180x180" href="/assets/images/govuk-apple-touch-icon-180x180.png">
    <link rel="apple-touch-icon" sizes="167x167" href="/assets/images/govuk-apple-touch-icon-167x167.png">
    <link rel="apple-touch-icon" sizes="152x152" href="/assets/images/govuk-apple-touch-icon-152x152.png">
    <link rel="apple-touch-icon" href="/assets/images/govuk-apple-touch-icon.png">
    <link rel="stylesheet" href="/assets/css/styles.css"/>
    <meta property="og:image" content="/assets/images/govuk-opengraph-image.png">
  </head>

  <body class="govuk-template__body ">

    <script src="/assets/js/jsEnabled.js" type="module" integrity="sha512-QtyPZKCSdWv9d3aKHJP6AL3dY/EbEFAAp4c6Hc/QOIvHcOXTu+P2TGqIlcIeieOvkWq1bKw3zehbKqH7Gv0fAQ==" crossorigin="anonymous"></script>

    {% include "_partials/skip-link.njk" %}
    {% include "_partials/header.njk" %}
    {% include "_partials/beta-banner.njk" %}
    {% include "_partials/primary-navigation.njk" %}

    <div class="govuk-body">
      <main
        class="govuk-main-wrapper govuk-main-wrapper--auto-spacing tfm-width govuk-!-padding-top-0"
        id="main-content"
      >

        {% if backLink and backLink.href %}
          {{ govukBackLink({
          text: "Back",
          href: backLink.href,
          attributes: {
            "data-cy": "back-link"
          }
        }) }}
        {% endif %}

        {% block content %}{% endblock %}
        {% block secondary_nav %}{% endblock %}
        {% block sub_content %}{% endblock %}
      </main>
    </div>

    {% include "_partials/footer.njk" %}

    <script src="/assets/js/main.js" type="module" integrity="sha512-BLhltBWOGogiNck88Bgk9ZP60WzkF0M4UDKNXASvMQ0XBcLRtMCqs77xMkkL+crGcCqpKtuOkkeuTvpTQFuzIA==" crossorigin="anonymous"></script>
    <script src="/assets/js/govukFrontend.js" type="module" integrity="sha512-Vk2b9tAD+b3UW0QRRQYYlVt7TYXoL25A8HrougokdITdzQLR6oWerqvbggafTBDYOtpO64j3n8zKjujsJkY91Q==" crossorigin="anonymous"></script>
    <script src="/assets/js/mojFrontend.js" type="module" integrity="sha512-Qk6Yl7rQlrEMujbcG1eTJ+aqZvUDj8FP4Xexy9FeU1SrY302u2vD7fqq3rq7BGvwhOcTdAlUKi1zmEOqHgU+1w==" crossorigin="anonymous"></script>
    <script src="/assets/js/disableFormSubmitOnSubmission.js" type="module" integrity="sha512-tCINlYTcYERel2bdysTMl5KaemDdEyog5JSS3hYQqHS6ZIyT4ATLu4oTPerbZ+vAOn4AAzDEsRBSQVacGhoieQ==" crossorigin="anonymous"></script>
    <script src="/assets/js/ssoRedirectAutoSubmit.js" type="module" integrity="sha512-YEZQKS3HeCijT1fixW7Q/tgkNdT1fzXDXPzTog2tCT04sw7xDpdHRSv5MTkD2UUsw0LCXf8Wkgp9XIaxFtSL+g==" crossorigin="anonymous"></script>

    {% block page_scripts %}{% endblock %}
  </body>
</html>
