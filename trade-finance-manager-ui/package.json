{
  "name": "tfm",
  "version": "1.0.0",
  "description": "Trade Finance Manager UI",
  "private": true,
  "main": "index.js",
  "engineStrict": true,
  "engines": {
    "node": ">=18",
    "npm": ">=9.5.0"
  },
  "scripts": {
    "lint": "eslint ./server ./scripts",
    "lint:fix": "eslint ./server ./scripts --fix",
    "build": "rimraf ./public/js && rimraf ./public/css && webpack --mode production --config webpack.prod.config.js",
    "start": "node server/index.js",
    "dev": "webpack --config webpack.config.dev.js --watch",
    "test": "jest --coverage --verbose --config=jest.config.js",
    "test-dev": "jest --verbose --config=jest.config.js",
    "test-quick": "jest --config=jest.config.js",
    "test-watch": "jest --watchAll --coverage --verbose --config=jest.config.js --detectOpenHandles",
    "component-test": "jest --config=jest.component-test.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs2.git"
  },
  "keywords": [
    "export",
    "finance"
  ],
  "author": "UK Export Finance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs2/issues"
  },
  "homepage": "https://github.com/UK-Export-Finance/dtfs2#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@ministryofjustice/frontend": "1.6.4",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "axios": "^1.3.4",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^3.5.3",
    "compression": "^1.7.4",
    "connect-flash": "0.1.1",
    "connect-redis": "^5.2.0",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.29.1",
    "csurf": "^1.11.0",
    "date-fns": "^2.29.3",
    "dotenv": "16.0.3",
    "express": "^4.18.2",
    "express-session": "1.17.3",
    "govuk-frontend": "4.3.1",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "imask": "^6.4.3",
    "joi": "^17.9.1",
    "jquery": "3.6.3",
    "lodash.isequal": "^4.5.0",
    "moment": "2.29.4",
    "moment-timezone": "^0.5.43",
    "morgan": "1.10.0",
    "node-fetch": "^2.6.9",
    "nodemon": "^2.0.22",
    "nunjucks": "3.2.3",
    "path": "0.12.7",
    "redis": "^3.1.2",
    "sanitize-html": "^2.10.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.20.2",
    "@babel/runtime": "7.20.7",
    "@types/jest": "^29.5.0",
    "babel-jest": "29.3.1",
    "babel-loader": "9.1.2",
    "chance": "^1.1.11",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "file-loader": "^6.2.0",
    "jest": "29.3.1",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "rimraf": "^4.4.1",
    "sass": "^1.60.0",
    "sass-loader": "13.2.0",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "^5.3.7",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0"
  }
