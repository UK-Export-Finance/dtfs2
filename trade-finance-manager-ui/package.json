{
  "name": "tfm-ui",
  "version": "1.0.0",
  "private": true,
  "description": "DTFS Trade Finance Manager UI",
  "keywords": [
    "export",
    "finance"
  ],
  "homepage": "https://github.com/UK-Export-Finance/dtfs2#readme",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs2.git"
  },
  "license": "MIT",
  "author": "UK Export Finance",
  "main": "index.ts",
  "scripts": {
    "api-test": "jest --coverage --verbose --forceExit --config=api-test.jest.config.js",
    "api-test-dev": "jest --verbose --forceExit --config=api-test.jest.config.js",
    "build": "rimraf ./public/js && rimraf ./public/css && webpack --mode production --config webpack.prod.config.js",
    "dev": "webpack --config webpack.dev.config.js --watch",
    "lint": "eslint --color **/*.{ts,js} --ignore-path ../.eslintignore",
    "lint:fix": "eslint --fix --color **/*.{ts,js}",
    "prettier": "prettier --no-error-on-unmatched-pattern --check **/*.{ts,js} --ignore-path ../.prettierignore",
    "prettier:fix": "prettier --write **/*.{ts,js} --ignore-path ../.prettierignore",
    "start": "npx ts-node server/index.ts",
    "type-check": "tsc --noEmit",
    "unit-test": "jest --coverage --verbose --config=unit.jest.config.js",
    "unit-test-dev": "jest --verbose --config=unit.jest.config.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@ministryofjustice/frontend": "1.8.0",
    "@types/lodash": "^4.17.4",
    "@ukef/dtfs2-common": "^1.0.0",
    "ajv": "^8.14.0",
    "axios": "^1.7.2",
    "chance": "^1.1.11",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^3.6.0",
    "compression": "^1.7.4",
    "connect-flash": "0.1.1",
    "connect-redis": "^5.2.0",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.37.1",
    "csurf": "^1.11.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.1",
    "dotenv": "16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-session": "1.17.3",
    "express-validator": "7.0.1",
    "express-xss-sanitizer": "^1.2.0",
    "govuk-frontend": "^4.8.0",
    "imask": "^6.6.3",
    "joi": "^17.13.1",
    "jquery": "3.6.4",
    "lodash.isequal": "^4.5.0",
    "lodash.orderby": "^4.6.0",
    "morgan": "1.10.0",
    "node-fetch": "^2.7.0",
    "nunjucks": "3.2.4",
    "path": "0.12.7",
    "redis": "^3.1.2",
    "sanitize-html": "^2.13.0",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.5",
    "@babel/plugin-transform-runtime": "7.21.4",
    "@babel/preset-env": "7.21.5",
    "@babel/runtime": "7.21.5",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.19.1",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/lodash.orderby": "^4.6.9",
    "@types/node": "^20.12.12",
    "axios-mock-adapter": "^1.22.0",
    "babel-jest": "29.5.0",
    "babel-loader": "9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "file-loader": "^6.2.0",
    "jest": "29.5.0",
    "node-mocks-http": "^1.14.1",
    "sass": "^1.77.2",
    "sass-loader": "13.2.2",
    "style-loader": "3.3.2",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.4",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "engineStrict": true
}
