{
  "name": "tfm",
  "version": "1.0.0",
  "description": "Trade Finance Manager UI",
  "private": true,
  "main": "index.js",
  "engineStrict": true,
  "engines": {
    "node": ">=18",
    "npm": ">=9.5.0"
  },
  "scripts": {
    "lint": "eslint ./server ./scripts",
    "lint:fix": "eslint ./server ./scripts --fix",
    "build": "rimraf ./public/js && rimraf ./public/css && webpack --mode production --config webpack.prod.config.js",
    "start": "node server/index.js",
    "dev": "webpack --config webpack.config.dev.js --watch",
    "test": "jest --coverage --verbose --config=jest.config.js",
    "test-dev": "jest --verbose --config=jest.config.js",
    "test-quick": "jest --config=jest.config.js",
    "test-watch": "jest --watchAll --coverage --verbose --config=jest.config.js --detectOpenHandles",
    "component-test": "jest --config=jest.component-test.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs2.git"
  },
  "keywords": [
    "export",
    "finance"
  ],
  "author": "UK Export Finance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs2/issues"
  },
  "homepage": "https://github.com/UK-Export-Finance/dtfs2#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@ministryofjustice/frontend": "1.6.6",
    "ajv": "^8.12.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "axios": "^1.4.0",
    "chance": "^1.1.11",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^3.5.3",
    "compression": "^1.7.4",
    "connect-flash": "0.1.1",
    "connect-redis": "^5.2.0",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.32.0",
    "csurf": "^1.11.0",
    "date-fns": "^2.30.0",
    "dotenv": "16.0.3",
    "express": "^4.18.2",
    "express-session": "1.17.3",
    "express-validator": "7.0.1",
    "express-xss-sanitizer": "^1.1.6",
    "govuk-frontend": "4.6.0",
    "graphql": "^16.7.1",
    "graphql-tag": "^2.12.6",
    "imask": "^6.6.3",
    "joi": "^17.9.2",
    "jquery": "3.6.4",
    "lodash.isequal": "^4.5.0",
    "moment": "2.29.4",
    "moment-timezone": "^0.5.43",
    "morgan": "1.10.0",
    "node-fetch": "^2.6.12",
    "nodemon": "^3.0.1",
    "nunjucks": "3.2.4",
    "path": "0.12.7",
    "redis": "^3.1.2",
    "sanitize-html": "^2.11.0",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.5",
    "@babel/plugin-transform-runtime": "7.21.4",
    "@babel/preset-env": "7.21.5",
    "@babel/runtime": "7.21.5",
    "@types/jest": "^29.5.3",
    "axios-mock-adapter": "^1.21.5",
    "babel-jest": "29.5.0",
    "babel-loader": "9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.0",
    "file-loader": "^6.2.0",
    "jest": "29.5.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "sass": "^1.64.1",
    "sass-loader": "13.2.2",
    "style-loader": "3.3.2",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0"
  },
  "imports": {
    "#api": "./server/api.js",
    "#constants": "./server/constants/index.js",
    "#constants/*": "./server/constants/*",
    "#controllers/*": "./server/controllers/*",
    "#controller-helpers": "./server/controllers/helpers/index.js",
    "#controller-helpers/*": "./server/controllers/helpers/*",
    "#helpers": "./server/helpers/index.js",//hmm this doesn't exist
    "#server-helpers/*": "./server/helpers/*",
    "#helpers/*": "./server/helpers/*",
    "#test-mocks": "./server/test-mocks/index.js",
    "#test-mocks/*": "./server/test-mocks/*",
    "#nunjucks-configuration/*": "./server/nunjucks-configuration/*",
    "#component-tests/*": "./component-tests/*"
  }
}
