{
  "name": "portal-ui",
  "version": "1.0.0",
  "private": true,
  "description": "DTFS Portal UI",
  "keywords": [
    "export",
    "finance"
  ],
  "homepage": "https://github.com/UK-Export-Finance/dtfs2#readme",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs2.git"
  },
  "license": "MIT",
  "author": "UK Export Finance",
  "main": "index.js",
  "scripts": {
    "api-test": "jest --coverage --verbose --forceExit --config=api-test.jest.config.js",
    "build": "rimraf ./public/js && rimraf ./public/css && webpack --mode production --config webpack.prod.config.js",
    "dev": "webpack --config webpack.config.dev.js --watch",
    "lint": "eslint ./server ./scripts ./component-tests ./test-helpers ./api-tests",
    "lint:fix": "eslint ./server ./scripts ./component-tests ./test-helpers ./api-tests --fix",
    "start": "node server/index.js",
    "test": "jest --coverage --verbose --config=jest.config.js",
    "test-dev": "jest --verbose --config=jest.config.js",
    "test-file": "jest --config=jest.config.js --verbose --testMatch",
    "test-quick": "jest --config=jest.config.js",
    "test-watch": "jest --watchAll --coverage --verbose --config=jest.config.js --detectOpenHandles"
  },
  "dependencies": {
    "@ministryofjustice/frontend": "1.8.0",
    "axios": "^1.6.0",
    "axios-mock-adapter": "^1.22.0",
    "cheerio": "^1.0.0-rc.12",
    "connect-flash": "0.1.1",
    "connect-redis": "5.2.0",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.33.2",
    "csurf": "^1.11.0",
    "dotenv": "16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-session": "1.17.3",
    "form-data": "4.0.0",
    "govuk-frontend": "4.6.0",
    "imask": "^6.6.3",
    "joi": "^17.11.0",
    "jquery": "^3.7.1",
    "json2csv": "^5.0.7",
    "lodash.isequal": "^4.5.0",
    "moment": "2.29.4",
    "moment-timezone": "0.5.43",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.0.1",
    "nunjucks": "3.2.4",
    "path": "0.12.7",
    "redis": "^3.1.2",
    "supertest": "^6.3.3",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.5",
    "@babel/plugin-transform-runtime": "7.21.4",
    "@babel/preset-env": "7.21.5",
    "babel-jest": "29.5.0",
    "babel-loader": "9.1.2",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "file-loader": "^6.2.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-when": "^3.6.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "sass-loader": "13.2.2",
    "style-loader": "3.3.2",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9.5.0"
  },
  "engineStrict": true
}
