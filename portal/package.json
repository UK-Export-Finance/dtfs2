{
  "name": "dtfs2",
  "version": "1.0.0",
  "description": "DTFS2 Portal",
  "private": true,
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./server ./scripts",
    "buildDev": "rm -rf dist && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js",
    "buildProd": "rm -rf dist && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "start": "npm run buildProd && node ./dist/server.js",
    "dev": "npm run buildDev && node ./dist/server.js",
    "test": "jest --notify --coverage --verbose --config=jest.config.js --detectOpenHandles",
    "test-quick": "jest --config=jest.config.js --detectOpenHandles",
    "test-watch": "jest --watchAll --notify --coverage --verbose --config=jest.config.js --detectOpenHandles",
    "component-test": "jest --notify --config=jest.component-test.config.js --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notbinary/dtfs2.git"
  },
  "keywords": [
    "export",
    "finance"
  ],
  "author": "David Carboni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/notbinary/dtfs2/issues"
  },
  "homepage": "https://github.com/notbinary/dtfs2#readme",
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "@babel/runtime": "7.10.1",
    "@ministryofjustice/frontend": "0.0.19-alpha",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.17",
    "axios": "0.19.2",
    "azure-storage": "2.10.3",
    "connect-flash": "0.1.1",
    "connect-redis": "^5.0.0",
    "core-js": "^2.6.11",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.10",
    "express-json2csv": "^0.3.6",
    "express-session": "1.17.0",
    "form-data": "3.0.0",
    "govuk-frontend": "3.13.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "imask": "^6.0.5",
    "lodash.isequal": "^4.5.0",
    "moment": "2.24.0",
    "moment-timezone": "0.5.31",
    "morgan": "1.10.0",
    "multer": "1.4.2",
    "node-fetch": "^2.6.1",
    "nunjucks": "3.2.1",
    "path": "0.12.7",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.10.1",
    "@babel/plugin-transform-runtime": "7.10.1",
    "@babel/preset-env": "7.10.1",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "cheerio": "1.0.0-rc.3",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "3.4.2",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.20.1",
    "file-loader": "^6.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^4.5.0",
    "jest": "26.1.0",
    "jquery": "3.5.0",
    "node-sass": "^4.14.1",
    "nunjucks-html-loader": "1.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "terser-webpack-plugin": "^5.0.0",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-middleware": "3.7.2",
    "webpack-glob-entries": "1.0.1",
    "webpack-hot-middleware": "2.25.0",
    "webpack-node-externals": "^2.5.2"
  }
