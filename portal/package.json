{
  "name": "dtfs2",
  "version": "1.0.0",
  "description": "DTFS2 Portal",
  "private": true,
  "main": "index.js",
  "engineStrict": true,
  "engines": {
    "node": ">=14.0.0 <15.0.0",
    "npm": ">=6.0.0 <7.0.0"
  },
  "scripts": {
    "lint": "eslint ./server ./scripts",
    "lint:fix": "eslint ./server ./scripts --fix",
    "build": "rimraf ./public/js && rimraf ./public/css && webpack --mode production --config webpack.prod.config.js",
    "start": "node server/index.js",
    "dev": "webpack --config webpack.config.dev.js --watch",
    "test": "jest --notify --coverage --verbose --config=jest.config.js",
    "test-quick": "jest --config=jest.config.js",
    "test-watch": "jest --watchAll --notify --coverage --verbose --config=jest.config.js --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs2.git"
  },
  "keywords": [
    "export",
    "finance"
  ],
  "author": "UK Export Finance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs2/issues"
  },
  "homepage": "https://github.com/UK-Export-Finance/dtfs2#readme",
  "dependencies": {
    "@ministryofjustice/frontend": "1.2.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.17",
    "axios": "^0.21.4",
    "connect-flash": "0.1.1",
    "connect-redis": "^5.0.0",
    "core-js": "^2.6.11",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "express-session": "1.17.0",
    "form-data": "3.0.0",
    "govuk-frontend": "4.0.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "helmet": "^5.0.1",
    "imask": "^6.0.5",
    "jquery": "^3.6.0",
    "json2csv": "^5.0.6",
    "lodash.isequal": "^4.5.0",
    "moment": "2.29.1",
    "moment-timezone": "0.5.31",
    "morgan": "1.10.0",
    "multer": "1.4.2",
    "node-fetch": "^2.6.1",
    "node-notifier": "^10.0.0",
    "nunjucks": "3.2.1",
    "path": "0.12.7",
    "redis": "^3.1.2",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.10.1",
    "@babel/plugin-transform-runtime": "7.10.1",
    "@babel/preset-env": "7.10.1",
    "babel-jest": "27.2.5",
    "babel-loader": "8.0.6",
    "cheerio": "^1.0.0-rc.10",
    "eslint": "^8.0.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.1",
    "file-loader": "^6.0.0",
    "jest": "27.2.1",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "sass": "^1.35.2",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "terser-webpack-plugin": "^5.2.5",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  }
}
