{
  "name": "external-apis",
  "version": "1.0.0",
  "description": "External APIs",
  "main": "src/index.ts",
  "engineStrict": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "start": "node src/index.ts",
    "start:dev": "npx nodemon src/index.ts",
    "api-test": "jest --notify --coverage --verbose --runInBand --config=api-test.jest.config.js",
    "api-test-file": "jest --config=api-test.jest.config.js --verbose --detectOpenHandles --testMatch",
    "api-test-quick": "jest --config=api-test.jest.config.js --runInBand --testTimeout=10000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs2.git"
  },
  "keywords": [
    "export",
    "finance"
  ],
  "author": "UK Export Finance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs2/issues"
  },
  "homepage": "https://github.com/UK-Export-Finance/dtfs2#readme",
  "dependencies": {
    "axios": "^0.26.0",
    "compression": "^1.7.4",
    "cors": "2.8.5",
    "cron-job-manager": "^2.2.1",
    "dotenv": "16.0.0",
    "elastic-apm-node": "^3.27.0",
    "express": "4.17.2",
    "helmet": "^5.0.2",
    "joi": "^17.6.0",
    "mongodb": "4.0.0",
    "node-cron": "^3.0.0",
    "node-notifier": "^10.0.0",
    "notifications-node-client": "^5.1.1",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "tslib": "^2.3.1",
    "winston": "^3.5.1",
    "winston-elasticsearch": "^0.16.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.21",
    "@types/node-cron": "^3.0.1",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.7",
    "nodemon": "^2.0.15",
    "openapi-types": "^10.0.0",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
