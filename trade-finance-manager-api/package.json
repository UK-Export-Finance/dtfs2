{
  "name": "tfm-api",
  "version": "1.0.0",
  "description": "Trade Finance Manager API",
  "main": "src/index.js",
  "engineStrict": true,
  "engines": {
    "node": ">=18",
    "npm": ">=9.5.0"
  },
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "start": "node src/index.js",
    "start:dev": "npx nodemon src/index.js",
    "unit-test": "jest --coverage --verbose --config=unit.jest.config.js ",
    "api-test": "jest --coverage --verbose --runInBand --forceExit --config=api-test.jest.config.js",
    "api-test-dev": "jest --verbose --runInBand --detectOpenHandles --config=api-test.jest.config.js ",
    "api-test-file": "jest --config=api-test-file.jest.config.js --runInBand --verbose --testMatch "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs2.git"
  },
  "keywords": [
    "export",
    "finance"
  ],
  "author": "UK Export Finance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs2/issues"
  },
  "homepage": "https://github.com/UK-Export-Finance/dtfs2#readme",
  "dependencies": {
    "@graphql-tools/schema": "^8.5.1",
    "apollo-server-express": "2.26.1",
    "apollo-server-testing": "^2.25.3",
    "axios": "^1.3.4",
    "bson": "^4.7.2",
    "comma-number": "^2.1.0",
    "compression": "^1.7.4",
    "cors": "2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^2.29.3",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "graphql": "^15.8.0",
    "graphql-middleware": "^4.0.3",
    "graphql-shield": "^7.6.5",
    "graphql-tag": "^2.12.6",
    "is-port-reachable": "^3.1.0",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.42",
    "mongo-dot-notation": "^3.1.0",
    "mongodb": "4.0.0",
    "node-cron": "^3.0.2",
    "nodemon": "^2.0.22",
    "sanitize-html": "^2.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/mongodb": "^4.0.7",
    "@types/swagger-ui-express": "^4.1.3",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "29.3.1",
    "supertest": "^6.3.3"
  }
}
