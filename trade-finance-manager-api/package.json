    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongo-dot-notation": "^3.1.0",
    "mongodb": "^4.17.2",
    "passport": "0.6.0",
    "passport-jwt": "^4.0.1",
