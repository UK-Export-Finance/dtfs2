    "axios-mock-adapter": "^2.1.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.9.1",
    "eslint-plugin-import": "^2.31.0",
    "jest": "29.5.0",
    "jest-when": "^3.7.0",
    "node-mocks-http": "^1.16.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=23.4.0",
    "npm": ">=10.8.2"
  },
  "engineStrict": true
