    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "jest": "29.5.0",
    "jest-when": "^3.6.0",
    "node-mocks-http": "^1.14.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.4",
    "typescript": "^5.4.5"
