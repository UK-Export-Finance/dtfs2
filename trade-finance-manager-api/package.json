{
  "name": "tfm-api",
  "version": "1.0.0",
  "description": "Trade Finance Manager API",
  "main": "src/index.js",
  "engineStrict": true,
  "engines": {
    "node": ">=14.0.0 <15.0.0",
    "npm": ">=6.0.0 <7.0.0"
  },
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "start": "node src/index.js",
    "start:dev": "npx nodemon src/index.js",
    "unit-test": "jest --notify --coverage --verbose --config=unit.jest.config.js ",
    "unit-test-quick": "jest --config=unit.jest.config.js --testTimeout=10000",
    "api-test": "jest --notify --coverage --verbose --runInBand --config=api-test.jest.config.js ",
    "api-test-file": "jest --config=api-test-file.jest.config.js --runInBand --verbose --testMatch ",
    "api-test-quick": "jest --config=api-test.jest.config.js --runInBand --testTimeout=10000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs2.git"
  },
  "keywords": [
    "export",
    "finance"
  ],
  "author": "UK Export Finance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs2/issues"
  },
  "homepage": "https://github.com/UK-Export-Finance/dtfs2#readme",
  "dependencies": {
    "@graphql-tools/schema": "^7.1.5",
    "@sentry/integrations": "^6.14.3",
    "@sentry/node": "^6.14.3",
    "apollo-server-express": "2.18.2",
    "apollo-server-testing": "^2.25.3",
