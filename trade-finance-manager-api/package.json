{
  "name": "tfm-api",
  "version": "1.0.0",
  "description": "Trade Finance Manager API",
  "main": "src/index.js",
  "engineStrict": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "start": "node src/index.js",
    "start:dev": "npx nodemon src/index.js",
    "unit-test": "jest --coverage --verbose --config=unit.jest.config.js ",
    "api-test": "jest --coverage --verbose --runInBand --forceExit --config=api-test.jest.config.js",
    "api-test-dev": "jest --verbose --runInBand --detectOpenHandles --config=api-test.jest.config.js ",
    "api-test-file": "jest --config=api-test-file.jest.config.js --runInBand --verbose --testMatch "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs2.git"
  },
  "keywords": [
    "export",
    "finance"
  ],
  "author": "UK Export Finance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs2/issues"
  },
  "homepage": "https://github.com/UK-Export-Finance/dtfs2#readme",
  "dependencies": {
    "@graphql-tools/schema": "^8.5.0",
    "apollo-server-express": "2.26.1",
    "apollo-server-testing": "^2.20.0",
    "axios": "^0.27.2",
    "bson": "^4.7.0",
    "comma-number": "^2.1.0",
    "compression": "^1.7.4",
    "cors": "2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^2.29.2",
    "dotenv": "16.0.2",
    "express": "4.18.1",
    "graphql": "^15.0.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.5.0",
    "graphql-tag": "^2.12.6",
    "is-port-reachable": "^3.0.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.37",
    "mongo-dot-notation": "^2.0.0",
    "mongodb": "4.0.0",
    "node-cron": "^3.0.2",
    "nodemon": "^2.0.19",
    "sanitize-html": "^2.7.1",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/swagger-ui-express": "^4.1.3",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "29.0.3",
    "supertest": "^6.2.4"
  }
}
