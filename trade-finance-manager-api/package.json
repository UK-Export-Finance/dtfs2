{
  "name": "tfm-api",
  "version": "1.0.0",
  "description": "DTFS Trade Finance Manager API",
  "keywords": [
    "export",
    "finance"
  ],
  "homepage": "https://github.com/UK-Export-Finance/dtfs2#readme",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs2.git"
  },
  "license": "MIT",
  "author": "UK Export Finance",
  "main": "src/index.js",
  "scripts": {
    "api-test": "jest --coverage --verbose --runInBand --forceExit --config=api-test.jest.config.js",
    "api-test-dev": "jest --verbose --runInBand --detectOpenHandles --config=api-test.jest.config.js ",
    "api-test-file": "jest --config=api-test-file.jest.config.js --runInBand --verbose --testMatch ",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "start": "node src/index.js",
    "start:dev": "npx nodemon src/index.js",
    "unit-test": "jest --coverage --verbose --config=unit.jest.config.js "
  },
  "lint-staged": {
    "package.json": "sort-package-json",
    "*.js": "eslint --fix",
    "*.ts": "eslint --fix",
    "*": [
      "cspell lint --gitignore --no-summary --fail-fast --no-must-find-files --color"
    ]
  },
  "dependencies": {
    "@graphql-tools/schema": "^9.0.19",
    "apollo-server-express": "3.12.0",
    "axios": "^1.5.0",
    "bson": "^4.7.2",
    "comma-number": "^2.1.0",
    "compression": "^1.7.4",
    "cors": "2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^2.30.0",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.11.0",
    "express-validator": "7.0.1",
    "graphql": "^16.8.0",
    "graphql-middleware": "^6.1.35",
    "graphql-shield": "^7.6.5",
    "graphql-tag": "^2.12.6",
    "is-port-reachable": "^3.1.0",
    "joi": "^17.10.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mongo-dot-notation": "^3.1.0",
    "mongodb": "4.2.0",
    "node-cron": "^3.0.2",
    "nodemon": "^3.0.1",
    "passport": "0.6.0",
    "passport-jwt": "^4.0.1",
    "sanitize-html": "^2.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/mongodb": "^4.0.7",
    "@types/swagger-ui-express": "^4.1.3",
    "axios-mock-adapter": "^1.21.5",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "jest": "29.5.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9.5.0"
  },
  "engineStrict": true
}
