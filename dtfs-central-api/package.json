{
  "name": "central-api",
  "version": "1.0.0",
  "description": "Central API",
  "main": "src/index.js",
  "engineStrict": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "start": "node src/index.js",
    "start:dev": "npx nodemon src/index.js",
    "unit-test": "jest --notify --coverage --verbose --config=unit.jest.config.js ",
    "unit-test-quick": "jest --config=unit.jest.config.js --testTimeout=10000",
    "api-test": "jest --notify --coverage --verbose --runInBand --detectOpenHandles --config=api-test.jest.config.js ",
    "api-test-file": "jest --config=api-test-file.jest.config.js --runInBand --verbose --testMatch ",
    "api-test-quick": "jest --config=api-test.jest.config.js --runInBand --testTimeout=10000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs2.git"
  },
  "keywords": [
    "export",
    "finance"
  ],
  "author": "UK Export Finance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs2/issues"
  },
  "homepage": "https://github.com/UK-Export-Finance/dtfs2#readme",
  "dependencies": {
    "@jest/test-sequencer": "^27.5.1",
    "axios": "^0.26.1",
    "compression": "^1.7.4",
    "cors": "2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "helmet": "^5.0.2",
    "moment": "^2.29.1",
    "mongo-dot-notation": "2.0.0",
    "mongodb": "~3.6.12",
    "node-notifier": "^10.0.1",
    "supertest": "6.2.2",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "27.5.1",
    "nodemon": "^2.0.15"
  }
}
