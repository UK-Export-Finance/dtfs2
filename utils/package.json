{
    "name": "dtfs-utilities",
    "version": "1.0.0",
    "description": "DTFS Utilities",
    "license": "MIT",
    "author": "UK Export Finance",
    "scripts": {
        "predb:seed": "rm -rf sql-db-seeder/node_modules && (cd sql-db-seeder && mkdir node_modules && cp -r ../../node_modules/typeorm-extension node_modules)",
        "db:seed": "npx ts-node sql-db-seeder/node_modules/typeorm-extension/bin/cli.cjs seed:run -r sql-db-seeder -d src/seeding-data-source.ts",
        "lint": "eslint --color **/*.{ts,js} --ignore-path ../.eslintignore",
        "lint:fix": "eslint --fix --color **/*.{ts,js}",
        "load": "ts-node mock-data-loader/re-insert-mocks.js",
        "prettier": "prettier --no-error-on-unmatched-pattern --check **/*.{ts,js}",
        "prettier:fix": "prettier --write **/*.{ts,js}",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@ukef/dtfs2-common": "^1.0.0",
        "axios": "^1.6.8",
        "bson": "^6.7.0",
        "chance": "^1.1.11",
        "date-fns": "^2.30.0",
        "dotenv": "^16.4.5",
        "generate-password": "^1.7.1",
        "html-entities": "^2.5.2",
        "json2csv": "^5.0.7",
        "jsonwebtoken": "^9.0.2",
        "minimist": "^1.2.8",
        "moment": "^2.30.1",
        "mongodb": "^6.3.0",
        "mssql": "^10.0.2",
        "reflect-metadata": "^0.2.2",
        "typeorm": "^0.3.20",
        "typeorm-extension": "^3.5.1",
        "xml2js": "^0.5.0",
        "zod": "^3.23.8"
    },
    "engines": {
        "node": ">=20",
        "npm": ">=10"
    },
    "engineStrict": true
}
