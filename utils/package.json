{
    "name": "dtfs-utilities",
    "version": "1.0.0",
    "private": true,
    "description": "DTFS Utilities",
    "license": "MIT",
    "author": "UK Export Finance",
    "scripts": {
        "predb:seed": "rm -rf sql-db-seeder/node_modules && (cd sql-db-seeder && mkdir node_modules && cp -r ../../node_modules/typeorm-extension node_modules)",
        "db:seed": "npx ts-node sql-db-seeder/node_modules/typeorm-extension/bin/cli.cjs seed:run -r sql-db-seeder -d src/seeding-data-source.ts",
        "db:seed:reset": "npx ts-node sql-db-seeder/src/reset-seeder.ts",
        "env:copy": "ts-node copy-env-vars/index.ts",
        "lint": "eslint --color **/*.{ts,js} --ignore-path ../.eslintignore",
        "lint:fix": "eslint --fix --color **/*.{ts,js}",
        "load": "ts-node mock-data-loader/re-insert-mocks.js",
        "prettier": "prettier --no-error-on-unmatched-pattern --check **/*.{ts,js} --ignore-path ../.prettierignore",
        "prettier:fix": "prettier --write **/*.{ts,js} --ignore-path ../.prettierignore",
        "type-check": "tsc --noEmit --pretty"
    },
    "dependencies": {
        "@faker-js/faker": "^8.4.1",
        "@ukef/dtfs2-common": "^1.0.0",
        "axios": "^1.7.2",
        "bson": "^6.8.0",
        "chance": "^1.1.12",
        "date-fns": "^2.30.0",
        "dotenv": "^16.4.5",
        "generate-password": "^1.7.1",
        "html-entities": "^2.5.2",
        "json2csv": "^5.0.7",
        "jsonwebtoken": "^9.0.2",
        "minimist": "^1.2.8",
        "moment": "^2.30.1",
        "mongodb": "^4.17.2",
        "mssql": "^10.0.4",
        "reflect-metadata": "^0.2.2",
        "typeorm": "^0.3.20",
        "typeorm-extension": "^3.5.1",
        "xml2js": "^0.5.0",
        "zod": "^3.23.8"
    },
    "engines": {
        "node": ">=20",
        "npm": ">=10"
    },
    "engineStrict": true
}
