{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "publicIPAddresses_tfs_dev_nat_ip_name": {
            "defaultValue": "tfs-dev-nat-ip",
            "type": "String"
        },
        "natGateways_tfs_dev_nat_gw_externalid": {
