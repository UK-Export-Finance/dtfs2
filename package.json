{
  "name": "dtfs2",
  "version": "1.0.0",
  "description": "DTFS2 Portal",
  "private": true,
  "main": "index.js",
  "scripts": {
    "buildDev": "rm -rf dist && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js",
    "buildProd": "rm -rf dist && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "start": "node ./dist/server.js",
    "dev": "npm run buildDev && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notbinary/dtfs2.git"
  },
  "keywords": [
    "export",
    "finance"
  ],
  "author": "David Carboni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/notbinary/dtfs2/issues"
  },
  "homepage": "https://github.com/notbinary/dtfs2#readme",
  "dependencies": {
    "express": "^4.17.1",
    "govuk-frontend": "^3.5.0",
    "nunjucks": "^3.2.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.1",
    "nunjucks-html-loader": "^1.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-glob-entries": "^1.0.1",
    "webpack-node-externals": "^1.7.2"
  }
}
