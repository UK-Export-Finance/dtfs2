{% from "govuk/components/back-link/macro.njk" import govukBackLink %}

<!DOCTYPE html>
<html lang="en" class="govuk-template govuk-template--rebranded">
  <head>
    <meta charset="utf-8"/>
    <title>
      {% block pageTitle %}{% endblock %}
      - UK Export Finance
    </title>
    <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover">
    <meta name="theme-color" content="#1d70b8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <link rel="shortcut icon" sizes="16x16 32x32 48x48" href="/assets/rebrand/images/govuk-icon-mask.svg" type="image/x-icon"/>
    <link rel="icon" sizes="48x48" href="/assets/rebrand/images/favicon.ico">
    <link rel="icon" sizes="any" href="/assets/rebrand/images/favicon.svg" type="image/svg+xml">
    <link rel="mask-icon" href="/assets/rebrand/images/govuk-icon-mask.svg" color="#1d70b8">
    <link rel="apple-touch-icon" href="/assets/rebrand/images/govuk-icon-180.png">
    <link rel="stylesheet" href="/assets/css/styles.css"/>
    <meta property="og:image" content="/assets/rebrand/images/govuk-opengraph-image.png">
    <link rel="manifest" href="/assets/rebrand/manifest.json">

    {% block head %}{% endblock %}
  </head>

  <body class="govuk-template__body govuk-body">

    <script
      src="/assets/js/jsEnabled.js"
      type="module"
      integrity="sha512-BZmKCksPLPXsFlYmd1MUEDLllt/d7vn1jLdU6FA1Y7hpzaOK7Aj9wwS3alwFEl+tlS5Md3CmwjI98F5Ggsg92Q=="
      crossorigin="anonymous"></script>

    {% include "_partials/header.njk" %}
    {% include "_partials/beta-banner.njk" %}

    <div class="govuk-width-container">
      <main class="govuk-main-wrapper govuk-main-wrapper--auto-spacing" id="main-content">
        {% if backLink and backLink.href %}
          {{ govukBackLink({
            text: "Back",
            href: backLink.href,
            attributes: {
              "data-cy": "back-link"
            }
          }) }}
        {% endif %}
        {% block content %}{% endblock %}
      </main>
    </div>

    {% include "_partials/footer.njk" %}

    <script
      src="/assets/js/main.js"
      type="module"
      integrity="sha512-Iwm3XauAflNW4AYmD2uqGEYHnqy4Id+3wd4ntbWyiPZtqlB5o2qzkrslmiGHWa+bJ06vum4zZCXYJsJa01htPw=="
      crossorigin="anonymous">
    </script>

    <script
      src="/assets/js/govukFrontend.js"
      type="module"
      integrity="sha512-o0KaKF4ummQGvu8rUZRscbd8CI0brZMQdjHrVNLD12TOUywTk1hiFRE89iyttfCGm9Ar44BqToAZBJHjKB2zGQ=="
      crossorigin="anonymous">
    </script>

    <script
      src="/assets/js/mojFrontend.js"
      type="module"
      integrity="sha512-oZACuErpjnaaxu4APOJyHBZAk/RW7M5gZ/hVPEBXmbVdcxcdiH89/ey/lqII6wTmpUv87g92RrelMbFXB8qBng=="
      crossorigin="anonymous">
    </script>

    <script
      src="/assets/js/maskedInputs.js"
      type="module"
      integrity="sha512-9fwf8xy1jVf1TvX8ex+8QhAiJfYPN18JXqxt2ULvQ5Y7kO8ZfPTfthqeL35unzb9zCmlpwhRQ6/g4rX6kc+zNQ=="
      crossorigin="anonymous">
    </script>

    <script
      src="/assets/js/mojFilters.js"
      type="module"
      integrity="sha512-ByfzBGRfJ1AM3hcN4bl0gILRnr3l9IDe8Um0poccVZ5qEfTpNj5r+rbYXQlEk1tL6zTdrIS2U77Kt4Jxi78Usw=="
      crossorigin="anonymous">
    </script>

    <script
      src="/assets/js/disableFormSubmitOnSubmission.js"
      type="module"
      integrity="sha512-mf1ibQA5lxNaMb/56mmj17voBUxRrxo3kx1vtkPz9+vGGookm+vqn8a/5l1fuu2exmAOaz1pLxxoMVZJAlYrFw=="
      crossorigin="anonymous">
    </script>

      {% block scripts %}{% endblock %}
  </body>
</html>
