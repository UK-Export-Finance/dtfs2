    "@ukef/dtfs2-common": "1.0.0",
    "axios": "1.12.0",
    "cheerio": "1.0.0-rc.12",
    "clamscan": "^2.4.0",
    "connect-flash": "0.1.1",
    "cookie-parser": "^1.4.7",
    "core-js": "^3.47.0",
    "csv-parser": "^3.2.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.1",
    "dotenv": "16.0.3",
    "exceljs": "^4.4.0",
    "express": "^4.22.1",
    "express-rate-limit": "^6.11.2",
    "form-data": "4.0.5",
    "govuk-frontend": "^5.13.0",
    "http-errors": "^2.0.1",
    "imask": "^6.6.3",
    "joi": "^17.13.3",
    "jquery": "^3.7.1",
    "json2csv": "^5.0.7",
    "lodash": "^4.17.23",
    "lodash.isequal": "^4.5.0",
    "morgan": "^1.10.1",
    "multer": "2.0.2",
    "node-fetch": "^2.7.0",
