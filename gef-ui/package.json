{
  "name": "gef-ui",
  "version": "1.0.0",
  "private": true,
  "description": "DTFS GEF UI",
  "keywords": [
    "export",
    "finance"
  ],
  "homepage": "https://github.com/UK-Export-Finance/dtfs2#readme",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs2.git"
  },
  "license": "MIT",
  "author": "UK Export Finance",
  "main": "index.ts",
  "scripts": {
    "api-test": "jest --coverage --verbose --forceExit --config=api-test.jest.config.js",
    "build": "rimraf ./public/js && rimraf ./public/css && webpack --mode production --config webpack.prod.config.js",
    "dev": "webpack --config webpack.dev.config.js --watch",
    "lint": "eslint ./server ./scripts ./component-tests ./test-helpers ./api-tests",
    "lint:fix": "eslint ./server ./scripts ./component-tests ./test-helpers ./api-tests --fix",
    "start": "npx ts-node server/index.ts",
    "test": "jest --coverage --verbose --config=jest.config.js",
    "test-dev": "jest --verbose --config=jest.config.js",
    "test-file": "jest --config=jest.config.js --verbose --testMatch",
    "test-quick": "jest --config=jest.config.js",
    "test-watch": "jest --watchAll --coverage --verbose --config=jest.config.js --detectOpenHandles"
  },
  "dependencies": {
    "@ministryofjustice/frontend": "2.0.0",
    "ajv": "^8.12.0",
    "axios": "^1.6.2",
    "chance": "^1.1.11",
    "cheerio": "^1.0.0-rc.12",
    "clean-deep": "^3.4.0",
    "comma-number": "^2.1.0",
    "compression": "^1.7.4",
    "connect-flash": "0.1.1",
    "connect-redis": "5.2.0",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.34.0",
    "csurf": "^1.11.0",
    "date-fns": "^2.30.0",
    "dotenv": "16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "express-rate-limit": "^6.11.2",
    "express-session": "1.17.3",
    "filesize": "9.0.11",
    "form-data": "4.0.0",
    "govuk-frontend": "4.6.0",
    "http-errors": "^2.0.0",
    "imask": "^6.6.3",
    "joi": "^17.11.0",
    "jquery": "3.6.4",
    "lodash": "^4.17.21",
    "moment": "2.29.4",
    "moment-timezone": "0.5.43",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.0.2",
    "nunjucks": "3.2.4",
    "path": "0.12.7",
    "redis": "^3.1.2",
    "sanitize-html": "^2.11.0",
    "tslib": "^2.6.2",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.5",
    "@babel/plugin-transform-runtime": "7.21.4",
    "@babel/preset-env": "7.21.5",
    "@babel/runtime": "7.21.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "babel-jest": "29.5.0",
    "babel-loader": "9.1.2",
    "cookie-signature": "^1.2.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "file-loader": "^6.2.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "sass-loader": "13.2.2",
    "style-loader": "3.3.2",
    "supertest": "^6.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "engineStrict": true
}
