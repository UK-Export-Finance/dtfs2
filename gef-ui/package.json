{
  "name": "dtfs2",
  "version": "1.0.0",
  "private": true,
  "description": "DTFS2 Portal",
  "keywords": [
    "export",
    "finance"
  ],
  "homepage": "https://github.com/UK-Export-Finance/dtfs2#readme",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs2.git"
  },
  "license": "MIT",
  "author": "UK Export Finance",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./public/js && rimraf ./public/css && webpack --mode production --config webpack.prod.config.js",
    "dev": "webpack --config webpack.config.dev.js --watch",
    "lint": "eslint ./server ./scripts",
    "lint:fix": "eslint ./server ./scripts --fix",
    "start": "node server/index.js",
    "test": "jest --coverage --verbose --config=jest.config.js",
    "test-file": "jest --config=jest.config.js --verbose --testMatch",
    "test-quick": "jest --config=jest.config.js",
    "test-watch": "jest --watchAll --coverage --verbose --config=jest.config.js --detectOpenHandles"
  },
  "lint-staged": {
    "package.json": "sort-package-json",
    "*.js": "eslint --fix",
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "@ministryofjustice/frontend": "1.6.4",
    "ajv": "^8.12.0",
    "axios": "^1.4.0",
    "chance": "^1.1.11",
    "cheerio": "^1.0.0-rc.12",
    "clean-deep": "^3.4.0",
    "comma-number": "^2.1.0",
    "compression": "^1.7.4",
    "connect-flash": "0.1.1",
    "connect-redis": "5.2.0",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.32.0",
    "csurf": "^1.11.0",
    "date-fns": "^2.30.0",
    "dotenv": "16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-session": "1.17.3",
    "filesize": "9.0.11",
    "form-data": "4.0.0",
    "govuk-frontend": "4.6.0",
    "http-errors": "^2.0.0",
    "imask": "^6.6.3",
    "joi": "^17.9.2",
    "jquery": "3.6.4",
    "lodash": "^4.17.21",
    "moment": "2.29.4",
    "moment-timezone": "0.5.43",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "node-fetch": "^2.6.12",
    "nodemon": "^3.0.1",
    "nunjucks": "3.2.4",
    "path": "0.12.7",
    "redis": "^3.1.2",
    "sanitize-html": "^2.11.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.5",
    "@babel/plugin-transform-runtime": "7.21.4",
    "@babel/preset-env": "7.21.5",
    "@babel/runtime": "7.21.5",
    "@types/jest": "^29.5.3",
    "babel-jest": "29.5.0",
    "babel-loader": "9.1.2",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.0",
    "file-loader": "^6.2.0",
    "jest": "29.5.0",
    "rimraf": "^5.0.1",
    "sass": "^1.64.1",
    "sass-loader": "13.2.2",
    "style-loader": "3.3.2",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9.5.0"
  },
  "engineStrict": true
}
