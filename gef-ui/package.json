{
  "name": "dtfs2",
  "version": "1.0.0",
  "description": "DTFS2 Portal",
  "private": true,
  "main": "index.js",
  "engineStrict": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint ./server ./scripts",
    "lint:fix": "eslint ./server ./scripts --fix",
    "build": "rimraf ./public/js && rimraf ./public/css && webpack --mode production --config webpack.prod.config.js",
    "start": "node server/index.js",
    "dev": "webpack --config webpack.config.dev.js --watch",
    "test": "jest --notify --coverage --verbose --config=jest.config.js",
    "test-file": "jest --config=jest.config.js --verbose --testMatch",
    "test-quick": "jest --config=jest.config.js",
    "test-watch": "jest --watchAll --notify --coverage --verbose --config=jest.config.js --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/dtfs2.git"
  },
  "keywords": [
    "export",
    "finance"
  ],
  "author": "UK Export Finance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/dtfs2/issues"
  },
  "homepage": "https://github.com/UK-Export-Finance/dtfs2#readme",
  "dependencies": {
    "@ministryofjustice/frontend": "1.2.0",
    "axios": "^0.26.1",
    "chance": "^1.1.8",
    "clean-deep": "^3.4.0",
    "comma-number": "^2.1.0",
    "compression": "^1.7.4",
    "connect-flash": "0.1.1",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.21.1",
    "csurf": "^1.11.0",
    "date-fns": "^2.28.0",
    "dotenv": "16.0.0",
    "express": "^4.17.3",
    "express-fileupload": "^1.3.1",
    "express-session": "1.17.2",
    "filesize": "^8.0.7",
    "form-data": "4.0.0",
    "govuk-frontend": "^4.0.1",
    "http-errors": "^2.0.0",
    "imask": "^6.4.2",
    "joi": "^17.6.0",
    "jquery": "3.6.0",
    "lodash": "^4.17.21",
    "moment": "2.29.2",
    "moment-timezone": "0.5.34",
    "morgan": "1.10.0",
    "multer": "1.4.4",
    "node-fetch": "^2.6.1",
    "node-notifier": "^10.0.1",
    "nunjucks": "3.2.3",
    "path": "0.12.7",
    "redis": "^3.1.2",
    "rimraf": "^3.0.2",
    "sanitize-html": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/runtime": "7.17.8",
    "@types/jest": "^27.4.1",
    "babel-jest": "27.5.1",
    "babel-loader": "8.2.3",
    "cheerio": "^1.0.0-rc.10",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "file-loader": "^6.2.0",
    "jest": "27.5.1",
    "nodemon": "^2.0.15",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "sass": "^1.49.9",
    "sass-loader": "12.6.0",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  }
}
